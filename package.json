{
  "name": "kibadist-ui",
  "version": "1.0.0",
  "description": "Contract-driven UI component generator with upgrade support",
  "keywords": [
    "cli",
    "ui",
    "components",
    "codegen",
    "contract",
    "button",
    "tailwind",
    "css-modules",
    "upgrade",
    "merge"
  ],
  "homepage": "https://github.com/kibadist/kibadist-ui#readme",
  "bugs": {
    "url": "https://github.com/kibadist/kibadist-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kibadist/kibadist-ui.git"
  },
  "license": "MIT",
  "author": "Maksym Ivashchenko",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "kibadist-ui": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/contracts dist/",
