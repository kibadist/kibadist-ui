    "kibadist-ui": "node dist/cli.js",
    "ku": "node dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  }
